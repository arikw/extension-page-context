{
    "extends": "eslint:recommended",
    
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "script",
        "ecmaFeatures": {
            "impliedStrict": true
        }
    },
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "indent": 0,
        "space-before-function-paren": 0,
        "eol-last": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-undef": "error",
        "semi": ["error", "always"],
        "no-console": 0,
        "no-useless-escape": 0,
        "no-empty": 0,
        "no-var": 0,
        "no-unused-vars": ["error", { "vars": "local", "args": "none" }]
    },
    "globals": {
        
    }
}